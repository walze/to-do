{
   "name": "server",
   "version": "0.0.0",
   "main": "index.js",
   "license": "MIT",
   "scripts": {
      "dev:start": "node -r tsconfig-paths/register -r ts-node/register src/index.ts",
      "dev": "nodemon src/index.ts",
      "prebuild": "rimraf dist",
      "build": "ts-node ./src -d ./dist --transforms typescript,imports",
      "gen": "graphql-codegen --config codegen.yml",
      "start": "ts-node src/index.ts",
      "typeorm": "node -r tsconfig-paths/register -r ts-node/register ./node_modules/.bin/typeorm",
      "test": "eslint . --ext .ts,.tsx -c ./.eslintrc.json",
      "test-fix": "npm run test -- --fix"
   },
   "dependencies": {
      "@graphql-codegen/cli": "1.15.4",
      "@graphql-codegen/introspection": "1.15.4",
      "@graphql-codegen/typescript": "^1.15.4",
      "@graphql-codegen/typescript-document-nodes": "1.15.4",
      "@graphql-codegen/typescript-graphql-request": "^1.15.4",
      "@graphql-codegen/typescript-mongodb": "1.15.4",
      "@graphql-codegen/typescript-resolvers": "1.15.4",
      "@types/node": "^14.0.13",
      "@types/ramda": "^0.27.6",
      "@typescript-eslint/eslint-plugin": "^3.4.0",
      "@typescript-eslint/parser": "^3.4.0",
      "apollo": "^2.28.3",
      "apollo-server": "^2.15.0",
      "eslint": "^7.3.1",
      "eslint-config-google": "^0.14.0",
      "eslint-config-standard": "^14.1.1",
      "eslint-plugin-import": "^2.21.2",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-promise": "^4.2.1",
      "eslint-plugin-standard": "^4.0.1",
      "express": "^4.17.1",
      "express-graphql": "^0.9.0",
      "fp-ts": "^2.6.6",
      "graphql-tools": "^6.0.10",
      "mysql": "^2.18.1",
      "nodemon": "^2.0.4",
      "ramda": "^0.27.0",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^3.0.2",
      "ts-node": "^8.10.2",
      "typeorm": "0.2.25",
      "typescript": "^3.9.5"
   }
}
