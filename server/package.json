{
   "name": "server",
   "version": "0.0.0",
   "main": "index.js",
   "license": "MIT",
   "scripts": {
      "start": "node -r tsconfig-paths/register -r ts-node/register src/index.ts",
      "dev": "nodemon src/index.ts",
      "prebuild": "rimraf dist",
      "build": "ts-node ./src -d ./dist --transforms typescript,imports",
      "gen": "graphql-codegen --config codegen.yml",
      "test": "eslint . --ext .ts,.tsx -c ./.eslintrc.json",
      "test-fix": "npm run test -- --fix"
   },
   "dependencies": {
      "@graphql-codegen/cli": "1.16.0",
      "@graphql-codegen/introspection": "1.16.0",
      "@graphql-codegen/typescript": "^1.16.0",
      "@graphql-codegen/typescript-document-nodes": "1.16.0",
      "@graphql-codegen/typescript-graphql-request": "^1.16.0",
      "@graphql-codegen/typescript-mongodb": "1.16.0",
      "@graphql-codegen/typescript-resolvers": "1.16.0",
      "@prisma/client": "^2.1.3",
      "@types/node": "^14.0.14",
      "@types/ramda": "^0.27.7",
      "@typescript-eslint/eslint-plugin": "^3.5.0",
      "@typescript-eslint/parser": "^3.5.0",
      "apollo": "^2.28.3",
      "apollo-server": "^2.15.1",
      "eslint": "^7.3.1",
      "eslint-config-google": "^0.14.0",
      "eslint-config-standard": "^14.1.1",
      "eslint-plugin-import": "^2.22.0",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-promise": "^4.2.1",
      "eslint-plugin-standard": "^4.0.1",
      "express": "^4.17.1",
      "express-graphql": "^0.9.0",
      "fp-ts": "^2.6.7",
      "graphql-tools": "^6.0.12",
      "mysql": "^2.18.1",
      "nodemon": "^2.0.4",
      "ramda": "^0.27.0",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^3.0.2",
      "ts-node": "^8.10.2",
      "@prisma/cli": "^2.1.3",
      "typescript": "^3.9.6"
   },
   "devDependencies": {}
}
